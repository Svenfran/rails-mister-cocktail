<h1>Cocktails</h1>


<div class="container">
  <div class="row">
    <% @cocktails.each do |cocktail| %>
      <div class="col-sm-4">
        <div class="card mb-3" style="width: 18rem;">
          <img class="card-img-top" src="data:image/jpeg;base64," alt="Card image cap">
          <div class="card-body">
            <h5 class="card-title"><%= cocktail.name%></h5>
            <p class="card-text"></p>
            
            <%= link_to 'Receip', cocktail_path(cocktail), class: "btn btn-primary" %>
          </div>
        </div>
      </div>
    <% end %>
  </div>
</div>
